<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.AMaptrack"
    android:versionCode="1"
    android:versionName="1.4" >
    
    <uses-sdk android:minSdkVersion="7" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >        
        
        <activity
            android:name=".MapTrack"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>        
        
        <activity android:name=".AMapView"
            	  android:label="@string/app_name"
            	  android:screenOrientation="portrait">            
        </activity>
        
        <activity android:name=".VehicleList"
            	  android:label="Vehilce List"
            	  android:screenOrientation="portrait">            
        </activity>
        <activity android:name=".AlarmInfo"
            	  android:label="Alarm Info"
            	  android:screenOrientation="portrait">            
        </activity>
        <activity android:name=".Login"
            	  android:screenOrientation="portrait"
            	  android:label="@string/app_name" >            
        </activity>
        
        <activity android:name=".UserInfoActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/Add" >            
        </activity>
        
        <activity android:name=".ConfigActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/Config" >            
        </activity>
        
        <activity android:name=".ModifyPsdActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/ModifyPsd" >            
        </activity>
        
        <activity android:name=".CarInfoActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/Add" >            
        </activity>
        
        <activity android:name=".MapShowVehicleActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/VehicleShow" >            
        </activity>
        
        <activity android:name=".ViewActivity"
            	  android:screenOrientation="portrait"
            	  android:label="@string/app_name" >            
        </activity>       
        <activity android:name=".PlaySetting"
            	  android:screenOrientation="portrait"
            	  android:label="@string/app_name" >
            
        </activity>      
        <activity android:name="PlayVehicleMap"
            	  android:screenOrientation="portrait"
            	  android:label="@string/app_name" >            
        </activity>  
        
    </application>
    
	<user-permission android:name="android.permission.ACCESS_FINE_LOCATION"></user-permission>		 
 	<uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"></uses-permission>
  	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>